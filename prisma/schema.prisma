// Setup database 
//      Source 1: https://medium.com/@2018.itsuki/postgresql-with-next-js-and-prisma-44f66a05378a
//      Source 2: https://vercel.com/guides/nextjs-prisma-postgres
//      Source 3: https://vercel.com/docs/storage/vercel-postgres/using-an-orm

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
}