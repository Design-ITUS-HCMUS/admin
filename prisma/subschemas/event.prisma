model Event {
  id            Int            @id @default(autoincrement())
  name          String         
  key           String         @unique
  tag           String[]
  start         DateTime       @db.Date
  end           DateTime?      @db.Date
  thumbnail     Bucket         @relation(fields: [bucketID], references: [id])
  bucketID      Int            @unique
  description   String
  url           String?
  status        Boolean        @default(true)
  accountEvents AccountEvent[]
}